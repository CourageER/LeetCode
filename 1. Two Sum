public class Solution {
    public int[] TwoSum(int[] nums, int target)
        {
            for (int i = 0; i < nums.Count(); i++)
            {
                List<int> result = new List<int>();
                result.Add(i);
                int temp = target - nums[i];
                for (int j = i + 1; j < nums.Count(); j++)
                {
                    if (temp == nums[j])
                    {
                        result.Add(j);
                        return result.ToArray();
                    }
                }
            }
            return new int[1];
        }
}
